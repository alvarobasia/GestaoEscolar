<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<BorderPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CourseModelController">
   <center>
      <AnchorPane fx:id="back" prefHeight="800.0" prefWidth="800.0" styleClass="back" stylesheets="@application.css" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="newCourse" layoutX="78.0" layoutY="268.0" mnemonicParsing="false" styleClass="btn" stylesheets="@application.css" text="Adicionar um curso">
               <font>
                  <Font name="Liberation Sans" size="22.0" />
               </font></Button>
            <Button fx:id="deleteAndChangeCourse" layoutX="78.0" layoutY="497.0" mnemonicParsing="false" styleClass="btn" stylesheets="@application.css" text="Alterar/excluir um curso">
               <font>
                  <Font name="Liberation Sans" size="22.0" />
               </font></Button>
            <Button fx:id="newSupplie" layoutX="78.0" layoutY="172.0" mnemonicParsing="false" styleClass="btn" stylesheets="@application.css" text="Adicionar uma materia">
               <font>
                  <Font name="Liberation Sans" size="22.0" />
               </font></Button>
            <Button fx:id="deleteAndChangeSupplie" layoutX="78.0" layoutY="380.0" mnemonicParsing="false" styleClass="btn" stylesheets="@application.css" text="Alterar/excluir uma materia">
               <font>
                  <Font name="Liberation Sans" size="22.0" />
               </font></Button>
            <Button fx:id="backButton" layoutX="78.0" layoutY="626.0" mnemonicParsing="false" onAction="#backToMenu" styleClass="close" stylesheets="@application.css" text="voltar" textFill="red">
               <font>
                  <Font name="Liberation Sans" size="22.0" />
               </font></Button>
         </children></AnchorPane>
   </center>
   <left>
      <Pane fx:id="lateralBar" prefHeight="800.0" prefWidth="270.0" style="-fx-background-color: #f27d12;" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
